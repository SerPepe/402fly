# Copy this file to .env and fill in your values

# Wallet that receives payments for this service
FLY402_PAYMENT_ADDRESS=YOUR_SERVICE_WALLET_ADDRESS

# Token mint for the asset you accept (use your native SPL token mint or wrapped SOL or USDC)
FLY402_TOKEN_MINT=YOUR_TOKEN_MINT_ADDRESS

# Network: solana-mainnet, solana-devnet, solana-testnet
FLY402_NETWORK=solana-devnet

# Optional: override the Solana RPC endpoint (leave blank to use network defaults)
FLY402_RPC_URL=

# Optional: fee capture wallet (defaults to the library's embedded fee wallet if unset)
FLY402_FEE_WALLET=
